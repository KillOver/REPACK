/* 主题卡片 */
.theme-panel {
  border-color: $panelHeaderBorderColor;
  background-color: $panelBackgroundColor;
}
.theme-panel > .panel-heading {
  color: $panelHeaderFontColor;
  background-color: $panelHeaderColor;
  border-color: $panelHeaderBorderColor;
}
.theme-panel > .panel-body {
  background-color: $panelBodyColor;
}