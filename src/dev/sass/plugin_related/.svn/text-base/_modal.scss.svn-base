/* 选项 */
.front-select-item {
  padding: 8px 15px 0;
}
.front-select-item > span {
  cursor: pointer;
}
.front-selected-btn-area {
  margin-bottom: 8px;
}
.front-selected-area {
  border-top: 1px solid #ddd;
}
.front-tab-content {
  height:250px;
  overflow-y: auto;
  border-left:solid 1px #ddd;
  border-right:solid 1px #ddd;
  border-bottom:solid 1px #ddd;
}
/* 搜索Tab */
#front-tab-search {
  display: none;
}


/* @group zTree  右边框有关*/
.dTreeNode{
  cursor:pointer;
}

#freeshare-memberselected
{
  list-style-type:none;
  cursor:default;
  border-top: 1px solid #dddddd;
}

#freeshare-memberselected >li{
  margin: 3% 3% -1% -7%;
}
#freeshare-memberselected >li >a{
  font-size: small;
  cursor:pointer;
}

#freeshare-memberNum{
  margin-bottom: 2%;
}
.freeshare-bottom-20{
  margin-bottom: 20px;
}
.freeshare-bottom-5{
  margin-bottom: 5px;
}
.freeshare-left-10{
  margin-left: 10px;
}
.freeshare-top-5{
  padding-top: 5px;
}

#freeshare-groupselected
{
  list-style-type:none;
  cursor:default;
  border-top: 1px solid #dddddd;
}

#freeshare-groupselected >li{
  margin: 2% 3% 3% -12%;
}
#freeshare-groupselected >li >a{
  font-size: small;
  cursor:pointer;
}

#freeshare-groupNum{
  margin-bottom: 2%;
}
/*框的大小有关*/
@media (min-width: 768px) {
  .front-search-field {
    display: block;
    visibility: visible;
  }
  .front-search-mobile {
    display: none;
    visibility: hidden;
  }
  #freeshare-modal-sm{
    width:600px !important;
  }
  #freeshare-modal-lg{
    width: 800px !important;
  }
  #freeshare-modal-xs{
    width: 300px !important;
  }
  #freeshare-modal-xxs{
    width: 200px !important;
  }
  .scroll-panel{
    height:auto;
    max-height:300px;
    overflow-y: auto;
  }
  .front-center-block{
    margin-top:100%;
  }
}
/*头部和底部*/
.modal-header {
  background-color:#f5f5f5;
  border-top-left-radius:10px;
  border-top-right-radius:10px;
}
.modal-footer {
  background-color:#f5f5f5;
  border-bottom-left-radius:10px;
  border-bottom-right-radius:10px;
}
/*图像*/
.front-imgbox {
  float:left;
  width:70px;
  height:70px;
  margin:18px;
  cursor:pointer;
  border:solid 1px #eee;
}
.front-imgbox img {
  width:68px;
  height:68px;
}
.front-imgbox-70 {
  float:left;
  width:70px;
  height:70px;
  margin:18px;
}
.front-imgbox-70 img {
  width:68px;
  height:68px;
  border:solid 1px #eee;
}
.front-imgbox-100 {
  float:left;
  width:100px;
  height:100px;
  margin:18px;
}

.front-imgbox-100 img {
  width:98px;
  height:98px;
  border:solid 1px #eee;
}

.front-imgbox-70 p, .front-imgbox-100 p {
  text-align:center;
}


#freeshare-diyImg {
  text-align:center;
  padding:50px;
}

#freeshare-photo_1 {
  width:110px;
  height:34px;
  margin-top:-34px;
  margin-left:112px;
  z-index:100;
  opacity:0;
  cursor:pointer;
}
/*复选框*/
.checkbox_float_left {
  float: left;
  width: 48%;
  padding-left: 3%;
}